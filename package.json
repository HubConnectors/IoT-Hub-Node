{
  "name": "iot-hub-node",
  "version": "0.0.1",
  "description": "基于node.js的通用物联网管理平台",
  "main": "src/app.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "test": "pm2 start ./src/config/pm2.config.js --only test",
    "prod": "pm2 start ./src/config/pm2.config.js --only production",
    "stop": "pm2 stop ./src/config/pm2.config.js",
    "delete": "pm2 delete ./src/config/pm2.config.js",
    "list": "pm2 list",
    "lint": "eslint . --ext .js,.ts,.json",
    "format": "prettier --config .prettierrc . --write",
    "prepare": "husky install",
    "cz": "cz",
    "opendb": "npx prisma studio --schema ./src/db/prisma/schema.prisma",
    "updatedb": "npx prisma migrate dev --schema ./src/db/prisma/schema.prisma",
    "updatePermission": "ts-node-dev ./src/utils/permission.ts",
    "addmodule": "ts-node ./src/script/createModule.ts"
  },
  "keywords": [],
  "author": "DuRuofu",
  "license": "ISC",
  "dependencies": {
    "@influxdata/influxdb-client": "^1.33.2",
    "@influxdata/influxdb-client-apis": "^1.33.2",
    "@prisma/client": "^5.12.1",
    "casbin": "^5.28.0",
    "casbin-prisma-adapter": "^1.5.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "figlet": "^1.7.0",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.0",
    "koa-body": "^6.0.1",
    "koa-mount": "^4.0.0",
    "koa-parameter": "^3.0.1",
    "koa-router": "^12.0.1",
    "koa-static": "^5.0.0",
    "koa2-connect-history-api-fallback": "^0.1.3",
    "koa2-cors": "^2.0.6",
    "koa2-swagger-ui": "^5.10.0",
    "log4js": "^6.9.1",
    "node-schedule": "^2.1.1",
    "pm2": "^5.3.0",
    "swagger-jsdoc": "^6.2.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@types/figlet": "^1.5.8",
    "@types/ip": "^1.1.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/koa": "^2.14.0",
    "@types/koa-mount": "^4.0.5",
    "@types/koa-router": "^7.4.8",
    "@types/koa-static": "^4.0.4",
    "@types/koa2-cors": "^2.0.5",
    "@types/log4js": "^2.3.5",
    "@types/node": "^20.11.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "commitizen": "^4.3.0",
    "commitlint": "^18.4.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.1",
    "prisma": "^5.12.1",
    "ts-node": "^10.9.2"
  }
}
